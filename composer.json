{
    "name": "littlegolden/link-decisions",
    "description": "Decide how to open post content links in a cool way.",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Billy Wilcosky",
            "role": "Creator"
        }
    ],
    "require": {
        "flarum/core": ">=0.1.0-beta.8 <0.1.0-beta.12"
    },
    "autoload": {
        "psr-4": {
            "littlegolden\\Link-Decisions\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Littlegolden Link Decisions",
            "icon": {
                "name": "fas fa-link",
                "backgroundColor": "#ddd",
                "color": "#000"
            }
        }
    }
}
